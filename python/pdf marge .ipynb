   "source": [
    "from PyPDF2 import PdfWriter\n",
    "import os\n",
    "\n",
    "marge=PdfWriter()\n",
    "file= [file for file in os.listdir()if file.endswith(\".pdf\")]\n",
    "\n",
    "for pdf in file:\n",
    "    marge.append(pdf)\n",
    "\n",
    "marge.write(\"marge_pdf.pdf\")\n",
    "marge.close "
